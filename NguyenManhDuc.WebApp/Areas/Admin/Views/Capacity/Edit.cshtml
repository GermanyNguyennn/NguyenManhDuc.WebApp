@using NguyenManhDuc.WebApp.Models
@model CapacityModel
@{
    ViewData["Title"] = "Sửa Dung Lượng";
}
<div class="">
    <form asp-action="Edit" enctype="multipart/form-data">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

        <div class="form-group">
            <label>Dung Lượng</label>
            <input asp-for="Capacity" class="form-control" />
            <span asp-validation-for="Capacity" class="text-danger" />
        </div>

        <div class="form-group">
            <button class="btn btn-warning btn-sm">Thêm</button>
            <a asp-action="Index" asp-controller="Capacity" class="btn btn-warning btn-sm">Quay Lại</a>
        </div>
    </form>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
